{
    "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "principalId": {
            "type": "array",
            "defaultValue": []
        },
        "appId": {
            "type": "string",
            "defaultValue": ""
        },
        "principalSecret": {
            "type": "securestring",
            "defaultValue": ""
        },
        "adoOrgName": {
            "type": "string",
            "defaultValue": ""
        },
        "adoProjectName": {
            "type": "string",
            "defaultValue": ""
        },
        "ghOrgName": {
            "type": "string",
            "defaultValue": ""
        },
        "ghRepoName": {
            "type": "string",
            "defaultValue": ""
        },
        "paToken": {
            "type": "securestring",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "variables": {

    },
    "resources": [
        {
            "type": "Microsoft.Resources/resourceGroups",
            "apiVersion": "2018-05-01",
            "name": "ao-pipeline-rg",
            "location": "[deployment().location]",
            "properties": {}
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "name": "new-name",
            "resourceGroup": "ao-pipeline-rg",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/resourceGroups/', 'ao-pipeline-rg')]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "contentVersion": "1.0.0.0",
                    "uri": "https://raw.githubusercontent.com/hjscherer/AO/dev/aoArm/aoCICDArm.json"
                },
                "parameters": {
                }
            }
        }
    ],
    "outputs": {
        "deployment": {
            "type": "string",
            "value": "[concat(deployment().name, ' has successfully deployed. Welcome to AlwaysOn!')]"
        }
    }
}